@#!/usr/bin/perl
# $Id: xref.perl,v 351.4 2005-02-24 15:38:26-08 - - $

map { $hash{lc $_} .= " $." } m/(\w+)/g while <>;
map { print "$_$hash{$_}\n" } sort keys %hash;

@


