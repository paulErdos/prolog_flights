@#!/usr/local/bin/perl -w
use strict;
use warnings;
# $Id: env.perl,v 351.2 2005-02-24 15:24:12-08 - - $
#
# NAME
#    env.perl - print out process environment variables
#

for my $var (sort keys %ENV) {
   print "$var => $ENV{$var}\n";
};
@


